            </Rectangle.Fill>
        </Rectangle>
        <Rectangle Name="picture5" Tag="easel" Width="160" Height="140" Canvas.Left="414" Canvas.Top="462">
            <Rectangle.Fill>
                <ImageBrush ImageSource="/img/Picture_preview_transparent.png" />
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle Name="picture6" Tag="easel" Width="160" Height="140" Canvas.Left="681" Canvas.Top="462">
            <Rectangle.Fill>
                <ImageBrush ImageSource="/img/Picture_preview_transparent.png" />
            </Rectangle.Fill>
        </Rectangle>

        <Rectangle Name="wallTop" Tag="wall" Height="60" Width="950" HorizontalAlignment="Center" VerticalAlignment="Top" Stroke="Black"/>
        <Rectangle Name="wallBottom" Tag="wall" Height="60" Width="950" Stroke="Black"/>
        <Rectangle Name="wallLeft" Tag="wall" Height="630" Width="12" HorizontalAlignment="Left" VerticalAlignment="Center" Stroke="Black"/>
        <Rectangle Name="wallRight" Tag="wall" Height="630" Width="20" HorizontalAlignment="Left" VerticalAlignment="Center" Stroke="Black"/>

        <Rectangle Name="TeleportToLocaltion2_1" Tag="teleport" Height="165" Canvas.Left="940" Canvas.Top="270" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" Fill="LightGray" />
        <Rectangle Name="TeleportToLocaltionBack" Tag="teleport" Height="165" Canvas.Top="260" Width="30" HorizontalAlignment="Left" VerticalAlignment="Top" Fill="LightGray" />

        <Rectangle x:Name="AreaEasel1" Tag="easel_area" Height="294" Canvas.Left="39" Canvas.Top="45" Width="381" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <Rectangle x:Name="AreaEasel2" Tag="easel_area" Height="294" Canvas.Left="307" Canvas.Top="45" Width="381" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Rectangle x:Name="AreaEasel3" Tag="easel_area" Height="294" Canvas.Left="573" Canvas.Top="45" Width="381" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Rectangle x:Name="AreaEasel4" Tag="easel_area" Height="261" Canvas.Left="61" Canvas.Top="398" Width="338" HorizontalAlignment="Left" VerticalAlignment="Center"/>
